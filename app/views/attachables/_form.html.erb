<%= simple_form_for(@attachable) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :attachable_type %>
    <%= f.input :file_file_name %>
    <%= f.input :file_content_type %>
    <%= f.input :file_file_size %>
    <%= f.input :file_updated_at %>
    <%= f.input :title %>
    <%= f.input :file_processing %>
    <%= f.input :file_fingerprint %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
