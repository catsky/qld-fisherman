<%= simple_form_for(@address) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :street %>
    <%= f.input :park %>
    <%= f.input :postcode %>
    <%= f.input :suburb %>
    <%= f.input :state %>
    <%= f.input :country %>
    <%= f.input :visible %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
