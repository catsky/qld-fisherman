<%= simple_form_for(@moment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :message_id %>
    <%= f.input :address_id %>
    <%= f.simple_fields_for :photos do |s| %>
      <%= f.input s.input :file%>
    <% end %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
